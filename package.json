{
  "name": "automatiqal-cli",
  "version": "0.0.14",
  "description": "CLI wrapper for automatiqal",
  "author": {
    "email": "info@informatiqal.com",
    "name": "Informatiqal @informatiqal",
    "url": "https://informatiqal.com"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "preferGlobal": true,
  "bin": {
    "automatiqal": "./bin/automatiqal.js"
  },
  "scripts": {
    "start": "node dist/app.js",
    "prestart": "npm run build-tsc",
    "build-tsc": "tsc --p tsconfig.debug.json",
    "build": "rollup -c",
    "watch": "rollup -cw",
    "debug": "tsc --sourcemap",
    "test": "set TS_NODE_PROJECT=tsconfig.test.json&mocha --require ts-node/register test/**/*.spec.ts",
    "docs": "npx typedoc src/index.doc.ts --name \"Automatiqal\" --excludePrivate --hideGenerator"
  },
  "keywords": [
    "qlik",
    "sense",
    "qlik-sense",
    "qlik sense",
    "saas api",
    "api",
    "saas",
    "automate",
    "cli"
  ],
  "files": [
    "dist",
    "*.json",
    "*.md",
    "LICENSE",
    "CHANGELOG.md",
    "!tsconfig.json",
    "!tsconfig.test.json",
    "!tsconfig.debug.json",
    "!renovate.json",
    "!test.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/informatiqal/automatiqal-cli.git"
  },
  "bugs": {
    "url": "https://github.com/informatiqal/automatiqal-cli/issues"
  },
  "homepage": "https://informatiqal.com/automatiqal-cli/",
  "devDependencies": {
    "@rollup/plugin-commonjs": "23.0.0",
    "@types/chai": "4.3.3",
    "@types/mocha": "10.0.0",
    "@types/node": "17.0.35",
    "chai": "4.3.6",
    "dotenv": "16.0.3",
    "mocha": "10.1.0",
    "nyc": "15.1.0",
    "rollup": "2.79.1",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-typescript2": "0.34.1",
    "ts-node": "10.9.1",
    "typedoc": "0.23.17",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "@p-mcgowan/minimist": "^2.1.0",
    "automatiqal": "^0.0.13",
    "js-yaml": "^4.1.0"
  }
}